{
  "name": "bk-electron",
  "version": "0.0.1",
  "description": "betterkonachan electron version",
  "main": "dist/main.js",
  "scripts": {
    "dist": "npm run main:build && npm run render:build && npm run copy && electron-builder --win --x64",
    "copy": "copyfiles -f ./assets/src/image/splash.svg dist",
    "render:dev": "webpack-dev-server --inline --config ./webpack/render.dev.js",
    "render:build": "webpack --config ./webpack/render.prod.js",
    "web:dev": "webpack-dev-server --inline --config ./webpack/web.dev.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "main:start": "electron .",
    "main:dev": "cross-env NODE_ENV=development electron .",
    "main:watch": "webpack --mode development --watch --config ./webpack/main.js",
    "main:build": "webpack --mode production --config ./webpack/main.js",
    "dev": "concurrently --kill-others \"npm run render:dev\" \"npm run main:watch\" \"npm run main:dev\""
  },
  "keywords": [
    "betterkonachan",
    "electron"
  ],
  "author": "franciscowxp@github.com",
  "build": {
    "appId": "BetterKonacha",
    "files": [
      "./dist/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@trodi/electron-splashscreen": "^0.3.4",
    "@types/cheerio": "^0.22.11",
    "@types/electron-window-state": "^2.0.33",
    "@types/react-icons": "^3.0.0",
    "@types/react-spinkit": "^3.0.5",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.3",
    "electron-dl": "^1.14.0",
    "electron-window-state": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flip-toolkit": "^6.5.7",
    "react-graceful-image": "^1.2.9",
    "react-icons": "^3.5.0",
    "react-perfect-scrollbar": "^1.4.4",
    "react-spinkit": "^3.0.0",
    "typescript": "^3.4.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.4.3",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "postcss-assets": "^5.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
