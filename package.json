{
  "name": "bk-electron",
  "version": "1.0.0",
  "description": "betterkonachan electron version",
  "main": "dist/main.js",
  "scripts": {
    "dist": "npx rimraf dist/* && npm run main:build && npm run render:build && npm run copy && electron-builder --win --x64",
    "convert": "convert -density 512x512 -background transparent ./assets/src/image/icon.svg -define icon:auto-resize -colors 512 ./assets/src/image/icon.ico",
    "copy": "copyfiles -f ./assets/src/image/splash.svg ./assets/src/image/icon.ico dist",
    "render:dev": "webpack-dev-server --inline --config ./webpack/render.dev.ts",
    "render:build": "webpack --config ./webpack/render.prod.ts",
    "lint": "eslint ./**/*.{ts,tsx,js}",
    "prettier": "prettier ./**/*.{ts,tsx,js,css} --write",
    "test": "jest ",
    "main:start": "electron .",
    "main:watch": "webpack --mode development --watch --config ./webpack/main.ts",
    "main:build": "webpack --mode production --config ./webpack/main.ts"
  },
  "keywords": [
    "betterkonachan",
    "electron"
  ],
  "author": "franciscowxp@github.com",
  "build": {
    "appId": "BetterKonacha",
    "files": [
      "./dist/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "dist/icon.ico"
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "electron-dl": "^1.14.0",
    "electron-window-state": "^5.0.3",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-perfect-scrollbar": "^1.5.3",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "^4.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.5.3",
    "rxjs-hooks": "^0.5.2",
    "rxjs-retry-delay": "^0.3.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.2",
    "@trodi/electron-splashscreen": "^0.3.4",
    "@types/cheerio": "^0.22.14",
    "@types/electron-window-state": "^2.0.33",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/ramda": "^0.26.34",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-spinkit": "^3.0.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.4.0",
    "@types/webpack-env": "^1.14.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "postcss-assets": "^5.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
