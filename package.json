{
  "name": "bk-electron",
  "version": "1.0.0",
  "description": "betterkonachan electron version",
  "main": "dist/main.js",
  "scripts": {
    "dist": "npx rimraf dist/* && npm run main:build && npm run render:build && npm run copy && electron-builder --win --x64",
    "copy": "copyfiles -f ./assets/src/image/splash.svg ./assets/src/image/icon.ico dist",
    "render:dev": "webpack-dev-server --inline --config ./webpack/render.dev.ts",
    "render:build": "webpack --config ./webpack/render.prod.ts",
    "lint": "eslint ./**/*.{ts,tsx,js}",
    "prettier": "prettier ./**/*.{ts,tsx,js,css} --write",
    "test": "jest ",
    "main:start": "electron .",
    "main:watch": "webpack --mode development --watch --config ./webpack/main.ts",
    "main:build": "webpack --mode production --config ./webpack/main.ts"
  },
  "keywords": [
    "betterkonachan",
    "electron"
  ],
  "author": "franciscowxp@github.com",
  "build": {
    "appId": "BetterKonacha",
    "files": [
      "./dist/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "dist/icon.ico"
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.3",
    "electron-dl": "^1.14.0",
    "electron-window-state": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.6.1",
    "react-perfect-scrollbar": "^1.5.2",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "^4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.4.4",
    "@trodi/electron-splashscreen": "^0.3.4",
    "@types/cheerio": "^0.22.11",
    "@types/electron-window-state": "^2.0.33",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-icons": "^3.0.0",
    "@types/react-spinkit": "^3.0.5",
    "@types/react-transition-group": "^2.9.1",
    "@types/webpack": "^4.4.31",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "postcss-assets": "^5.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
